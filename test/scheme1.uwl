                in-sequence.angle:180;
                out-sequence.angle:360;
            }
            write-annotations {
                pos:"-176.667 -822.667";
                style:ext;
                bg-color-ext:"0 128 128 64";
                in-annotations.angle:180;
            }
            write-sequence {
                pos:"-180 -960";
                style:ext;
                bg-color-ext:"0 128 128 64";
                in-sequence.angle:180;
            }
            write-sequence-1 {
                pos:"-179.333 -1110.67";
