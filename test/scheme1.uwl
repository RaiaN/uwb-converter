#@UGENE_WORKFLOW



workflow "First scheme"{

    reverse-complement {
        type:reverse-complement;
        name:"Обратная комплементарность";
    }
    read-sequence {
        type:read-sequence;
        name:"Чтение последовательности";
        url-in {
            dataset:"Dataset 1";
            file:/home/raian/ugene-1.14.2/data/samples/FASTA/human_T1.fa;
        }
    }
    dna-stats {
        type:dna-stats;
        name:"ДНК статистика";
    }
    write-sequence {
        type:write-sequence;
        name:"Запись последовательности";
        url-out:outp1;
    }
    write-annotations {
        type:write-annotations;
        name:"Запись аннотаций";
        url-out:outp2;
    }
    reverse-complement-1 {
        type:reverse-complement;
        name:"Обратная комплементарность 1";
    }
    write-sequence-1 {
        type:write-sequence;
        name:"Запись последовательности";
        url-out:outp3;
    }

    .actor-bindings {
        reverse-complement.out-sequence->write-sequence.in-sequence
        reverse-complement.out-sequence->reverse-complement-1.in-sequence
        read-sequence.out-sequence->reverse-complement.in-sequence
        read-sequence.out-sequence->dna-stats.in-sequence
        dna-stats.out-annotations->write-annotations.in-annotations
        reverse-complement-1.out-sequence->write-sequence-1.in-sequence
    }

    read-sequence.sequence->reverse-complement.in-sequence.sequence
    read-sequence.sequence->dna-stats.in-sequence.sequence
    read-sequence.annotations->write-sequence.in-sequence.annotations
    reverse-complement.sequence->write-sequence.in-sequence.sequence
    dna-stats.annotations->write-annotations.in-annotations.annotations
    read-sequence.annotations->write-annotations.in-annotations.annotations
    read-sequence.sequence->write-annotations.in-annotations.sequence
    reverse-complement.sequence->reverse-complement-1.in-sequence.sequence
    read-sequence.annotations->write-sequence-1.in-sequence.annotations
    reverse-complement-1.sequence->write-sequence-1.in-sequence.sequence

    .meta {
        visual {
            dna-stats {
                pos:"-520 -798.667";
                style:ext;
                bg-color-ext:"0 128 128 64";
