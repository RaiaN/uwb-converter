                out-annotations.angle:360;
            }
            read-sequence {
                pos:"-735 -909";
                style:ext;
                bg-color-ext:"0 128 128 64";
                bounds:"-30 -30 91.25 84";
                out-sequence.angle:360;
            }
            reverse-complement {
